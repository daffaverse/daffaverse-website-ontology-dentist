                                      owl:unionOf ( :Gejala
                                                    :KondisiGigi
                                                    :PenyakitGigi
                                                  )
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty :berkembangMenjadi ;
