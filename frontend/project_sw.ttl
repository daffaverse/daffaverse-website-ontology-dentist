@base <http://www.semanticweb.org/iban/ontologies/2025/10/spesialis-gigi-recommender/> .

<http://www.semanticweb.org/iban/ontologies/2025/10/spesialis-gigi-recommender> rdf:type owl:Ontology .

#################################################################
#    Annotation properties
#################################################################

###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
<http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/iban/ontologies/2025/10/spesialis-gigi-recommender/berkaitanDengan
:berkaitanDengan rdf:type owl:ObjectProperty ,
                          owl:SymmetricProperty ;
                 rdfs:domain :KondisiGigi ;
                 rdfs:range :KondisiGigi .


###  http://www.semanticweb.org/iban/ontologies/2025/10/spesialis-gigi-recommender/berkembangMenjadi
:berkembangMenjadi rdf:type owl:ObjectProperty ,
                            owl:AsymmetricProperty ,
                            owl:IrreflexiveProperty ;
                   rdfs:domain :PenyakitGigi ,
                               [ rdf:type owl:Class ;
                                 owl:unionOf ( :Gejala
                                               :KondisiGigi
                                               :PenyakitGigi
                                             )
                               ] ;
                   rdfs:range :PenyakitGigi .


###  http://www.semanticweb.org/iban/ontologies/2025/10/spesialis-gigi-recommender/dialamiOleh
:dialamiOleh rdf:type owl:ObjectProperty ;
             owl:inverseOf :mengalamiGejala ;
             rdfs:domain :Gejala ;
             rdfs:range :Pasien .


###  http://www.semanticweb.org/iban/ontologies/2025/10/spesialis-gigi-recommender/didugaMenderita
:didugaMenderita rdf:type owl:ObjectProperty ;
                 rdfs:domain :Pasien ;
                 rdfs:range :PenyakitGigi ,
                            [ rdf:type owl:Class ;
                              owl:unionOf ( :Gejala
                                            :KondisiGigi
                                            :PenyakitGigi
                                          )
                            ] .


###  http://www.semanticweb.org/iban/ontologies/2025/10/spesialis-gigi-recommender/dipicuOleh
:dipicuOleh rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            rdfs:domain [ rdf:type owl:Class ;
                          owl:unionOf ( :Gejala
                                        :KondisiGigi
                                        :PenyakitGigi
                                      )
                        ] ;
            rdfs:range :Pemicu .


###  http://www.semanticweb.org/iban/ontologies/2025/10/spesialis-gigi-recommender/membutuhkanSpesialis
:membutuhkanSpesialis rdf:type owl:ObjectProperty ;
                      owl:inverseOf :menanganiPenyakit ;
                      rdfs:domain :PenyakitGigi ,
                                  [ rdf:type owl:Class ;
                                    owl:unionOf ( :Gejala
                                                  :KondisiGigi
                                                  :PenyakitGigi
                                                )
                                  ] ;
                      rdfs:range :SpesialisGigi .


###  http://www.semanticweb.org/iban/ontologies/2025/10/spesialis-gigi-recommender/memilikiGejala
:memilikiGejala rdf:type owl:ObjectProperty ;
                rdfs:domain :PenyakitGigi ,
                            [ rdf:type owl:Class ;
                              owl:unionOf ( :Gejala
                                            :KondisiGigi
                                            :PenyakitGigi
                                          )
                            ] ;
                rdfs:range :Gejala .


###  http://www.semanticweb.org/iban/ontologies/2025/10/spesialis-gigi-recommender/menanganiPenyakit
:menanganiPenyakit rdf:type owl:ObjectProperty ;
                   rdfs:domain :SpesialisGigi ;
                   rdfs:range :PenyakitGigi ,
                              [ rdf:type owl:Class ;
                                owl:unionOf ( :Gejala
                                              :KondisiGigi
                                              :PenyakitGigi
                                            )
                              ] .


###  http://www.semanticweb.org/iban/ontologies/2025/10/spesialis-gigi-recommender/mengalamiGejala
:mengalamiGejala rdf:type owl:ObjectProperty ;
                 rdfs:domain :Pasien ;
                 rdfs:range :Gejala .


###  http://www.semanticweb.org/iban/ontologies/2025/10/spesialis-gigi-recommender/mengalamiKondisi
:mengalamiKondisi rdf:type owl:ObjectProperty ;
                  rdfs:domain :Pasien ;
                  rdfs:range :KondisiGigi .


###  http://www.semanticweb.org/iban/ontologies/2025/10/spesialis-gigi-recommender/menyebabkan
:menyebabkan rdf:type owl:ObjectProperty ,
                      owl:TransitiveProperty ;
             rdfs:domain :KondisiGigi ;
             rdfs:range :PenyakitGigi ,
                        [ rdf:type owl:Class ;
                          owl:unionOf ( :Gejala
                                        :KondisiGigi
                                        :PenyakitGigi
                                      )
                        ] .


###  http://www.semanticweb.org/iban/ontologies/2025/10/spesialis-gigi-recommender/tingkatUrgensi
:tingkatUrgensi rdf:type owl:ObjectProperty ;
                rdfs:domain :PenyakitGigi ,
                            [ rdf:type owl:Class ;
                              owl:unionOf ( :Gejala
                                            :KondisiGigi
                                            :PenyakitGigi
                                          )
                            ] ;
                rdfs:range :Gejala .


